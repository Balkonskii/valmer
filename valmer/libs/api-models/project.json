{
    "projectType": "library",
    "sourceRoot": "libs/api-models/src",
    "prefix": "vm",
    "targets": {
        "test": {
            "executor": "@nrwl/jest:jest",
            "outputs": ["coverage/libs/api-models"],
            "options": {
                "jestConfig": "libs/api-models/jest.config.ts",
                "passWithNoTests": true
